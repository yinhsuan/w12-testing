{
  "name": "@nrwl/angular",
  "version": "14.0.5",
  "description": "The Nx Plugin for Angular contains executors, generators, and utilities for managing Angular applications and libraries within an Nx workspace. It provides: \n\n- Integration with libraries such as Storybook, Jest, ESLint, Tailwind CSS, Cypress, Karma, and Protractor. \n\n- Generators to help scaffold code quickly (like: Micro Frontends, Libraries, both internal to your codebase and publishable to npm) \n\n- Upgrading AngularJS applications \n\n- Single Component Application Modules (SCAMs) \n\n- NgRx helpers. \n\n- Utilities for automatic workspace refactoring.",
  "repository": {
    "type": "git",
    "url": "https://github.com/nrwl/nx.git",
    "directory": "packages/angular"
  },
  "scripts": {
    "postinstall": "node ./scripts/nx-cli-warning.js"
  },
  "keywords": [
    "Monorepo",
    "Angular",
    "Jest",
    "Cypress",
    "CLI"
  ],
  "exports": {
    "./generators": "./generators.js",
    "./executors": "./executors.js",
    "./tailwind": "./tailwind.js",
    "./src/generators/utils": "./src/generators/utils/index.js",
    "./module-federation": "./module-federation/index.js",
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./nrwl-angular.d.ts",
      "esm2020": "./esm2020/nrwl-angular.mjs",
      "es2020": "./fesm2020/nrwl-angular.mjs",
      "es2015": "./fesm2015/nrwl-angular.mjs",
      "node": "./fesm2015/nrwl-angular.mjs",
      "default": "./fesm2020/nrwl-angular.mjs"
    },
    "./mfe": {
      "types": "./mfe/nrwl-angular-mfe.d.ts",
      "esm2020": "./esm2020/mfe/nrwl-angular-mfe.mjs",
      "es2020": "./fesm2020/nrwl-angular-mfe.mjs",
      "es2015": "./fesm2015/nrwl-angular-mfe.mjs",
      "node": "./fesm2015/nrwl-angular-mfe.mjs",
      "default": "./fesm2020/nrwl-angular-mfe.mjs"
    },
    "./testing": {
      "types": "./testing/nrwl-angular-testing.d.ts",
      "esm2020": "./esm2020/testing/nrwl-angular-testing.mjs",
      "es2020": "./fesm2020/nrwl-angular-testing.mjs",
      "es2015": "./fesm2015/nrwl-angular-testing.mjs",
      "node": "./fesm2015/nrwl-angular-testing.mjs",
      "default": "./fesm2020/nrwl-angular-testing.mjs"
    }
  },
  "author": "Victor Savkin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nrwl/nx/issues"
  },
  "homepage": "https://nx.dev",
  "builders": "./executors.json",
  "schematics": "./generators.json",
  "ng-update": {
    "requirements": {},
    "migrations": "./migrations.json"
  },
  "dependencies": {
    "@angular-devkit/schematics": "~13.3.0",
    "@nrwl/cypress": "14.0.5",
    "@nrwl/devkit": "14.0.5",
    "@nrwl/jest": "14.0.5",
    "@nrwl/linter": "14.0.5",
    "@nrwl/storybook": "14.0.5",
    "@nrwl/workspace": "14.0.5",
    "@schematics/angular": "~13.3.0",
    "@phenomnomnominal/tsquery": "4.1.1",
    "chalk": "4.1.0",
    "chokidar": "^3.5.1",
    "ignore": "^5.0.4",
    "jasmine-marbles": "~0.8.4",
    "rxjs-for-await": "0.0.2",
    "webpack-merge": "5.7.3",
    "ts-node": "~9.1.1",
    "tsconfig-paths": "^3.9.0",
    "semver": "7.3.4",
    "webpack": "^5.58.1",
    "tslib": "^2.3.0"
  },
  "module": "fesm2015/nrwl-angular.mjs",
  "es2020": "fesm2020/nrwl-angular.mjs",
  "esm2020": "esm2020/nrwl-angular.mjs",
  "fesm2020": "fesm2020/nrwl-angular.mjs",
  "fesm2015": "fesm2015/nrwl-angular.mjs",
  "typings": "nrwl-angular.d.ts",
  "sideEffects": false
}